{
  "expo": {
    "name": "Paddle Rating",
    "slug": "PaddleratingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "paddlerating",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.paddlerating.app",
      "associatedDomains": [
        "applinks:paddlerating.app",
        "applinks:www.paddlerating.app"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.paddlerating.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "paddlerating.app",
              "pathPrefix": "/court"
            },
            {
              "scheme": "https",
              "host": "www.paddlerating.app",
              "pathPrefix": "/court"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "4995e305-c390-4198-a735-4a639e9eb025"
      },
      "deepLink": {
        "webDomain": "paddlerating.app",
        "appScheme": "paddlerating"
      },
      "branch": {
        "apiKey": "YOUR_BRANCH_LIVE_KEY",
        "testMode": true
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4995e305-c390-4198-a735-4a639e9eb025"
    }
  }
}
